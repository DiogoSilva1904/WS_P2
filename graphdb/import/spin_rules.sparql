#UrbanCenter true

PREFIX : <http://localhost:8000/ontology#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

INSERT {
?city :UrbanCenter true .
}
WHERE {
?city a :City ;
        :population ?pop .
FILTER(xsd:integer(?pop) > 10000)
}


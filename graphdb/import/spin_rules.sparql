#Inferir Weapon 

PREFIX ont: <http://localhost:8000/ontology#>
INSERT {
    ?x a ont:Weapon .
}
WHERE {
    ?x ont:type ?type .
    FILTER(
        CONTAINS(LCASE(STR(?type)), "melee") ||
        CONTAINS(LCASE(STR(?type)), "blaster") ||
        CONTAINS(LCASE(STR(?type)), "explosive") ||
        CONTAINS(LCASE(STR(?type)), "projectile")
    )
}



